                  "name": "Create cohort",
                  "request": {
                    "method": "POST",
                    "header": [],
                    "body": {
                      "mode": "raw",
                      "raw": "{\n    \"name\": \"High Blood Pressure Patients\",\n    \"description\": \"Patients with systolic BP > 140\",\n    \"filters\": {\n        \"visit\": {},\n        \"person_demographics\": {},\n        \"provider_demographics\": {},\n        \"clinical\": {}\n    },\n    \"visit_count\": 234\n}",
                      "options": {
