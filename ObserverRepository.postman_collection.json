            {
              "name": "Cohort",
              "description": "Cohort management endpoints. Users can create, update, and delete their own cohorts for filtering research data.",
              "item": [
                {
                  "name": "List cohorts",
                  "request": {
                    "method": "GET",
                    "header": [],
                    "url": {
                      "raw": "{{base_url}}/api/v1/accounts/cohorts/",
